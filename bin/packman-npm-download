#!/usr/bin/env node

const { initializeCommand } = require('../lib/core/commandInitializer');

const NpmDownloadPackageCommand = require('../lib/npm/download/NpmDownloadPackageCommand');
const NpmDownloadPackageLockCommand = require('../lib/npm/download/NpmDownloadPackageLockCommand');
const NpmDownloadPackageJsonCommand = require('../lib/npm/download/NpmDownloadPackageJsonCommand');
const NpmDownloadSearchCommand = require('../lib/npm/download/NpmDownloadSearchCommand');
const NpmDownloadAllCommand = require('../lib/npm/download/NpmDownloadAllCommand');
const NpmDownloadGenerateCommand = require('../lib/npm/download/NpmDownloadGenerateCommand');
const NpmDownloadFromGeneratedCommand = require('../lib/npm/download/NpmDownloadFromGeneratedCommand');

const nestedCommands = [
    new NpmDownloadPackageCommand(),
    new NpmDownloadPackageLockCommand(),
    new NpmDownloadPackageJsonCommand(),
    new NpmDownloadSearchCommand(),
    new NpmDownloadAllCommand(),
    new NpmDownloadGenerateCommand(),
    new NpmDownloadFromGeneratedCommand(),
];

initializeCommand('download', nestedCommands);
