#!/usr/bin/env node

const { initializeCommand } = require('../lib/core/initializer');

const NpmCopyPackageCommand = require('../lib/npm/copy/NpmCopyPackageCommand');
const NpmCopyPackageLockCommand = require('../lib/npm/copy/NpmCopyPackageLockCommand');
const NpmCopyPackageJsonCommand = require('../lib/npm/copy/NpmCopyPackageJsonCommand');
const NpmCopySearchCommand = require('../lib/npm/copy/NpmCopySearchCommand');
const NpmCopyAllCommand = require('../lib/npm/copy/NpmCopyAllCommand');

const nestedCommands = [
    new NpmCopyPackageCommand(),
    new NpmCopyPackageLockCommand(),
    new NpmCopyPackageJsonCommand(),
    new NpmCopySearchCommand(),
    new NpmCopyAllCommand(),
];

initializeCommand('copy', nestedCommands);
