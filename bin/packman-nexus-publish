#!/usr/bin/env node

const { initializeCommand } = require('../lib/core/initializer');

const NexusPublishTarballsCommand = require('../lib/nexus/publish/NexusPublishTarballsCommand');

const nestedCommands = [
    new NexusPublishTarballsCommand(),
];

const definitions = nestedCommands.map(c => c.definition);

initializeCommand('publish', definitions);
