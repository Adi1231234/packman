#!/usr/bin/env node

const { initializeCommand } = require('../lib/core/initializer');

const NugetDownloadAllCommand = require('../lib/nuget/download/NugetDownloadAllCommand');

const nestedCommands = [
    new NugetDownloadAllCommand(),
];

const definitions = nestedCommands.map(c => c.definition);

initializeCommand('download', definitions);
