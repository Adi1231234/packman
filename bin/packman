#!/usr/bin/env node

const { launchProgram, setLoggerVerbosity } = require('../src/core/commandInitializer');

setLoggerVerbosity();

const npm = require('../src/providers/npm/npmDefinition');
const nuget = require('../src/providers/nuget/nugetDefinition');
const nexus = require('../src/providers/nexus/nexusDefinition');
const bundle = require('../src/providers/bundle/bundleDefinition');

const definitions = [
    npm,
    nuget,
    nexus,
    bundle,
];

launchProgram(definitions);
