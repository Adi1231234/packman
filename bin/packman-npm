#!/usr/bin/env node

const { initializeCommand } = require('../lib/core/commandInitializer');

const definitions = [
    {
        name: 'download',
        description: 'Download package tarballs',
        alias: 'd',
    },
    {
        name: 'publish',
        description: 'Publish package tarballs',
        alias: 'p',
    },
    {
        name: 'copy',
        description: 'Download packages from one registry and publish to another',
        alias: 'c',
    },
];

initializeCommand('npm', definitions);
