{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "npm download package",
            "program": "${workspaceFolder}/bin/packman-npm-download",
            "args": [
                "package",
                "node-tgz-downloader",
                "--devDependencies"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "npm download search",
            "program": "${workspaceFolder}/bin/packman-npm-download",
            "args": [
                "search",
                "tgz",
                "--devDependencies"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "npm download local package-lock",
            "program": "${workspaceFolder}/bin/packman-npm-download",
            "args": [
                "package-lock",
                "./package-lock.json",
                "--directory",
                "custom-tarballs"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "npm download remote package-lock",
            "program": "${workspaceFolder}/bin/packman-npm-download",
            "args": [
                "package-lock",
                "https://raw.githubusercontent.com/BoundlessCode/packman/master/package-lock.json"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "npm download local package-json",
            "program": "${workspaceFolder}/bin/packman-npm-download",
            "args": [
                "package-json",
                "./package.json",
                "--devDependencies"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "npm download remote package-json",
            "program": "${workspaceFolder}/bin/packman-npm-download",
            "args": [
                "package-json",
                "https://raw.githubusercontent.com/BoundlessCode/packman/master/package.json",
                "--devDependencies"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "npm download all",
            "program": "${workspaceFolder}/bin/packman-npm-download",
            "args": [
                "all"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "jasmine Tests",
            "program": "${workspaceFolder}/node_modules/jasmine/bin/jasmine.js",
            "args": [
                "test/**/*spec.js",
                "--reporter=jasmine-console-reporter"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "npm copy package to fake registry",
            "program": "${workspaceFolder}/bin/packman-npm-copy",
            "args": [
                "package",
                "lodash",
                "--source",
                "https://registry.npmjs.org/",
                "--target",
                "https://fake.registry/"
            ],
            "skipFiles": [
                "${workspaceFolder}/node_modules/**/*.js",
                "<node_internals>/**/*.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "npm copy package to current registry",
            "program": "${workspaceFolder}/bin/packman-npm-copy",
            "args": [
                "package",
                "lodash",
                "--source",
                "https://registry.npmjs.org/",
            ],
            "skipFiles": [
                "${workspaceFolder}/node_modules/**/*.js",
                "<node_internals>/**/*.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "npm copy search to current registry",
            "program": "${workspaceFolder}/bin/packman-npm-copy",
            "args": [
                "search",
                "jquery",
                "--source",
                "https://registry.npmjs.org/",
            ],
            "skipFiles": [
                "${workspaceFolder}/node_modules/**/*.js",
                "<node_internals>/**/*.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "npm copy all to current registry",
            "program": "${workspaceFolder}/bin/packman-npm-copy",
            "args": [
                "all",
                "--source",
                "http://localhost:4873/",
            ],
            "skipFiles": [
                "${workspaceFolder}/node_modules/**/*.js",
                "<node_internals>/**/*.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "nuget --help",
            "program": "${workspaceFolder}/bin/packman-nuget",
            "args": [
                "--help",
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "nexus --help",
            "program": "${workspaceFolder}/bin/packman-nexus",
            "args": [
                "--help",
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "nuget download --help",
            "program": "${workspaceFolder}/bin/packman-nuget",
            "args": [
                "download",
                "--help",
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "nuget download all",
            "program": "${workspaceFolder}/bin/packman-nuget-download",
            "args": [
                "all",
                "--directory",
                "/Volumes/NomadicQuad/sharvit/packages/nuget-test",
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "nuget download all --track",
            "program": "${workspaceFolder}/bin/packman-nuget-download",
            "args": [
                "all",
                "--directory",
                "/Volumes/NomadicQuad/sharvit/packages/nuget-test",
                "--track",
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "nuget download all --help",
            "program": "${workspaceFolder}/bin/packman-nuget-download",
            "args": [
                "all",
                "--help",
            ]
        },
    ]
}